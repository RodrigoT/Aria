#: src/main.cc:108
msgid "Usage: aria [OPTION]..."
msgstr "Uso: aria [OPCIONES]..."

#: src/main.cc:109
msgid "Yet another download tool"
msgstr "Otra utilidad para descargar archivos"

#. command line options
#: src/main.cc:112
msgid "Options:"
msgstr "Opciones:"

#: src/main.cc:113
msgid "  -g, --get URL                    URL to get"
msgstr "  -g, --get URL                    URL del archivo a descargar"

#: src/main.cc:114
msgid "  -d, --dir SAVEDIR                save directory"
msgstr "  -d, --dir DIRECTORIO          directorio donde se grabará"

#: src/main.cc:115
msgid "  -r, --ref REFERER                referer"
msgstr "  -r, --ref REFERER                referer"

#: src/main.cc:116
msgid "  -u, --url-list FILE              open URL list"
msgstr "  -u, --url-list ARCHIVO           abrir lista de URLs"

#: src/main.cc:117
msgid "  -c, --crc-list FILE              open CRC list"
msgstr "  -c, --crc-list ARCHIVO           abrir lista de CRSs"

#: src/main.cc:118
msgid "  --basket                         Show basket and iconify Aria"
msgstr "  --basket                       Miostrar canasta y minimizar Aria"

#: src/main.cc:119
msgid "  -v, --version                    print version information"
msgstr "  -v, --version                    mostrar información de la versión"

#: src/main.cc:120
msgid "  -h, --help                       print this help"
msgstr "  -h, --help                       mostrar esta ayuda"

#: src/main.cc:121
msgid "  --initrc                         initialize resource files"
msgstr "  --initrc                         inicializar archivo de recursos"

#: src/main.cc:122
msgid ""
"  --geometry WIDTHxHEIGHT+XOFF+YOFF\n"
"                                   set the size and position of main window"
msgstr ""
"  --geometry ANCHOxALTO+POSX+POSY\n"
"                                   fijar posición y tamaño de la ventana "
"principal"

#: src/main.cc:123
msgid ""
"  --thread-limit-override NUM      if you want more simultaneous downloads..."
msgstr ""
"  --thread-limit-override NUM      cantidad de archivos simultáneos a "
"descargar"

#: src/main.cc:125
msgid ""
"Control Options:\n"
" the following options are effective only when another aria is already "
"running"
msgstr ""
"Opciones de control:\n"
" las siguientes opciones tienen efecto sólo cuando ya hay otro aria corriendo"

#: src/main.cc:127
msgid "  -s, --start                      start all downloads"
msgstr "  -s, --start                      iniciar todas las descargas"

#: src/main.cc:128
msgid "  -p, --stop                       stop all downloads"
msgstr "  -p, --stop                       detener todas las descargas"

#: src/main.cc:129
msgid "  -a, --again                      re-download all items"
msgstr "  -a, --again                      descargar de nuevo todos los items"

#: src/main.cc:130
msgid "  -q, --quit                       quit Aria(with no confirmation)"
msgstr ""
"  -q, --quit                       salir de Aria (sin pedir confirmación)"

#: src/main.cc:131
msgid "  --list-name                      show the name of the current list"
msgstr ""
"  --list-name                      mostrar el nombre de la lista actual"

#: src/main.cc:132
msgid "  --list-prev                      switch to the previous list"
msgstr "  --list-prev                      cambiar a la lista anterior"

#: src/main.cc:133
msgid "  --list-next                      switch to the next list"
msgstr "  --list-next                      cambiar a la lista siguiente"

#: src/main.cc:134
msgid "  --list-delete                    delete the current list"
msgstr "  --list-delete                    borrar la lista actual"

#: src/main.cc:135
msgid "  --clear-complete                 clear downloaded items"
msgstr ""
"  --clear-complete                 borrar de la lista los items recibidos"

#: src/main.cc:136
msgid "  --ls-complete                    show URLs downloaded"
msgstr "  --ls-complete                    mostrar las URLs descargadas"

#: src/main.cc:137
msgid "  --ls-ready                       show URLs waiting in queue"
msgstr "  --ls-ready                       Mostrar las URLs que estan en cola"

#: src/main.cc:138
msgid "  --ls-error                       show URLs in error"
msgstr "  --ls-error                       mostrar las URLs con errores"

#: src/main.cc:139
msgid "  --ls-run                         show URLs now downloading"
msgstr ""
"  --ls-run                         mostrar las URLs que se estan descargando"

#: src/main.cc:140
msgid "  --ls-stop                        show URLs stopped"
msgstr ""
"  --ls-stop                        mostrar las URLs que estan detenidas"

#: src/main.cc:141
msgid "  --ls-lock                        show URLs locked"
msgstr ""
"  --ls-lock                        mostrar las URLs que estan bloqueadas"

#: src/main.cc:142
msgid "  --ls-all                         show all URLs in queue"
msgstr ""
"  --ls-all                         mostrar todas las URLs que estan en cola"

