#. command line options
#: src/main.cc:112
msgid "Options:"
msgstr "Opzioni:"

#: src/main.cc:113
msgid "  -g, --get URL                    URL to get"
msgstr "  -g, --get URL                    URL da scaricare"

#: src/main.cc:114
msgid "  -d, --dir SAVEDIR                save directory"
msgstr "  -d, --dir SAVEDIR                directory di salvataggio"

#: src/main.cc:115
msgid "  -r, --ref REFERER                referer"
msgstr "  -r, --ref REFERER                referer"

#: src/main.cc:116
msgid "  -u, --url-list FILE              open URL list"
msgstr "  -u, --url-list FILE              apre lista di URL"

#: src/main.cc:117
msgid "  -c, --crc-list FILE              open CRC list"
msgstr "  -c, --crc-list FILE              apre lista di CRC"

#: src/main.cc:118
msgid "  --basket                         Show basket and iconify Aria"
msgstr "  --basket                         mostra cestino e iconizza Aria"

#: src/main.cc:119
msgid "  -v, --version                    print version information"
msgstr ""
"  -v, --version                    stampa le informazioni sulla versione"

#: src/main.cc:120
msgid "  -h, --help                       print this help"
msgstr "  -h, --help                       mostra questo aiuto"

#: src/main.cc:121
msgid "  --initrc                         initialize resource files"
msgstr "  --initrc                         inizializza i file di risorsa"

#: src/main.cc:122
msgid ""
"  --geometry WIDTHxHEIGHT+XOFF+YOFF\n"
"                                   set the size and position of main window"
msgstr ""
"  --geometry WIDTHxHEIGHT+XOFF+YOFF\n"
"                                   imposta dimensione e posizione della "
"finestra principale"

#: src/main.cc:123
msgid ""
"  --thread-limit-override NUM      if you want more simultaneous downloads..."
msgstr ""
"  --thread-limit-override NUM      se vuoi più scaricamenti simultanei..."

#: src/main.cc:125
msgid ""
"Control Options:\n"
" the following options are effective only when another aria is already "
"running"
msgstr ""
"Opzioni di Controllo:\n"
" le seguenti opzioni hanno effetto solo quando un altro aria è già in "
"esecuzione"

#: src/main.cc:127
msgid "  -s, --start                      start all downloads"
msgstr "  -s, --start                      avvia tutti gli scaricamenti"

#: src/main.cc:128
msgid "  -p, --stop                       stop all downloads"
msgstr "  -p, --stop                       ferma tutti gli scaricamenti"

#: src/main.cc:129
msgid "  -a, --again                      re-download all items"
msgstr ""
"  -a, --again                      scarica nuovamente tutti gli elementi"

#: src/main.cc:130
msgid "  -q, --quit                       quit Aria(with no confirmation)"
msgstr "  -q, --quit                       chiude Aria (senza conferma)"

#: src/main.cc:131
msgid "  --list-name                      show the name of the current list"
msgstr "  --list-name                      mostra il nome della lista corrente"

#: src/main.cc:132
msgid "  --list-prev                      switch to the previous list"
msgstr "  --list-prev                      passa alla lista precente"

#: src/main.cc:133
msgid "  --list-next                      switch to the next list"
msgstr "  --list-next                      passa alla lista successiva"

#: src/main.cc:134
msgid "  --list-delete                    delete the current list"
msgstr "  --list-delete                    cancella la lista corrente"

#: src/main.cc:135
msgid "  --clear-complete                 clear downloaded items"
msgstr "  --clear-complete                 elimina elementi scaricati"

#: src/main.cc:136
msgid "  --ls-complete                    show URLs downloaded"
msgstr "  --ls-complete                    mostra URL scaricati"

#: src/main.cc:137
msgid "  --ls-ready                       show URLs waiting in queue"
msgstr "  --ls-ready                       mostra URL in attesa in coda"

#: src/main.cc:138
msgid "  --ls-error                       show URLs in error"
msgstr "  --ls-error                       mostra URL in errore"

#: src/main.cc:139
msgid "  --ls-run                         show URLs now downloading"
msgstr "  --ls-run                         mostra URL in scaricamento adesso"

#: src/main.cc:140
msgid "  --ls-stop                        show URLs stopped"
msgstr "  --ls-stop                        mostra URL fermati"

#: src/main.cc:141
msgid "  --ls-lock                        show URLs locked"
msgstr "  --ls-lock                        mostra URL bloccati"

#: src/main.cc:142
msgid "  --ls-all                         show all URLs in queue"
msgstr "  --ls-all                         mostra tutti gli URL in coda"

