#. command line options
#: src/main.cc:112
msgid "Options:"
msgstr "オプション:"

#: src/main.cc:113
msgid "  -g, --get URL                    URL to get"
msgstr "  -g, --get URL                    URL をダウンロード"

#: src/main.cc:114
msgid "  -d, --dir SAVEDIR                save directory"
msgstr "  -d, --dir SAVEDIR                保存先ディレクトリ"

#: src/main.cc:115
msgid "  -r, --ref REFERER                referer"
msgstr "  -r, --ref REFERER                リファラー"

#: src/main.cc:116
msgid "  -u, --url-list FILE              open URL list"
msgstr "  -u, --url-list FILE              URL リストファイルを開く"

#: src/main.cc:117
msgid "  -c, --crc-list FILE              open CRC list"
msgstr "  -c, --crc-list FILE              CRC リストファイルを開く"

#: src/main.cc:118
msgid "  --basket                         Show basket and iconify Aria"
msgstr ""
"  --ls-basket                        バスケットを表示し、Aria を最小化する"

