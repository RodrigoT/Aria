msgid "  -h, --help                       print this help"
msgstr "  -h, --help                       このヘルプを表示"

#: src/main.cc:121
msgid "  --initrc                         initialize resource files"
msgstr "  --initrc                         設定ファイルを初期化"

#: src/main.cc:122
msgid ""
"  --geometry WIDTHxHEIGHT+XOFF+YOFF\n"
"                                   set the size and position of main window"
msgstr ""
"  --geometry WIDTHxHEIGHT+XOFF+YOFF\\n                                    メ"
"インウインドウの位置とサイズを指定する"

#: src/main.cc:123
msgid ""
"  --thread-limit-override NUM      if you want more simultaneous downloads..."
msgstr "  --thread-limit-override NUM      より多くの同時ダウンロード数を指定"

#: src/main.cc:125
msgid ""
"Control Options:\n"
" the following options are effective only when another aria is already "
"running"
msgstr ""
"コントロールオプション:\n"
" 以下のオプションは Aria がすでに同一ユーザに実行されている場合にのみ有効です"

#: src/main.cc:127
msgid "  -s, --start                      start all downloads"
msgstr "  -s, --start                      全てのダウンロードを開始"

#: src/main.cc:128
msgid "  -p, --stop                       stop all downloads"
msgstr "  -p, --stop                       全てのダウンロードを停止"

#: src/main.cc:129
msgid "  -a, --again                      re-download all items"
msgstr "  -a, --again                      全てのダウンロードをやり直す"

#: src/main.cc:130
msgid "  -q, --quit                       quit Aria(with no confirmation)"
msgstr "  -q, --quit                       Aria を終了させる"

#: src/main.cc:131
msgid "  --list-name                      show the name of the current list"
msgstr "  --list-name                      現在のリストの名前を表示"

#: src/main.cc:132
msgid "  --list-prev                      switch to the previous list"
msgstr "  --list-prev                      前のリストに切替え"

#: src/main.cc:133
msgid "  --list-next                      switch to the next list"
msgstr "  --list-next                      次のリストに切替え"

#: src/main.cc:134
msgid "  --list-delete                    delete the current list"
msgstr "  --list-delete                    現在のリストを削除"

#: src/main.cc:135
msgid "  --clear-complete                 clear downloaded items"
msgstr "  --clear-complete                 ダウンロード済アイテムを削除する"

#: src/main.cc:136
msgid "  --ls-complete                    show URLs downloaded"
msgstr "  --ls-complete                    ダウンロード済の URL を表示する"

#: src/main.cc:137
msgid "  --ls-ready                       show URLs waiting in queue"
msgstr "  --ls-ready                       待機中の URL を表示する"

#: src/main.cc:138
msgid "  --ls-error                       show URLs in error"
msgstr "  --ls-error                       エラーの URL を表示する"

#: src/main.cc:139
msgid "  --ls-run                         show URLs now downloading"
msgstr "  --ls-run                         ダウンロード中の URL を表示する"

#: src/main.cc:140
msgid "  --ls-stop                        show URLs stopped"
msgstr "  --ls-stop                        停止中の URL を表示する"

#: src/main.cc:141
msgid "  --ls-lock                        show URLs locked"
msgstr "  --ls-lock                        ロックされた URL を表示する"

#: src/main.cc:142
msgid "  --ls-all                         show all URLs in queue"
msgstr "  --ls-all                         すべての URL を表示する"

