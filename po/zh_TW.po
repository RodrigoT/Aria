#: src/main.cc:108
msgid "Usage: aria [OPTION]..."
msgstr "提示: aria [選項]..."

#: src/main.cc:109
msgid "Yet another download tool"
msgstr "Yet another download tool"

#. command line options
#: src/main.cc:112
msgid "Options:"
msgstr "選項:"

#: src/main.cc:113
msgid "  -g, --get URL                    URL to get"
msgstr "  -g, --get URL                    取得 URL"

#: src/main.cc:114
msgid "  -d, --dir SAVEDIR                save directory"
msgstr "  -d, --dir SAVEDIR                儲存於目錄.."

#: src/main.cc:115
msgid "  -r, --ref REFERER                referer"
msgstr "  -r, --ref REFERER                參考"

#: src/main.cc:116
msgid "  -u, --url-list FILE              open URL list"
msgstr "  -u, --url-list FILE              開啟 URL 列表"

#: src/main.cc:117
msgid "  -c, --crc-list FILE              open CRC list"
msgstr "  -c, --crc-list FILE              開啟 CRC 列表"

#: src/main.cc:118
msgid "  --basket                         Show basket and iconify Aria"
msgstr "  --basket                         顯示浮動圖示並最小化 Aria"

#: src/main.cc:119
msgid "  -v, --version                    print version information"
msgstr "  -v, --version                    列出版本訊息"

#: src/main.cc:120
msgid "  -h, --help                       print this help"
msgstr "  -h, --help                       列出本協助訊息"

#: src/main.cc:121
msgid "  --initrc                         initialize resource files"
msgstr "  --initrc                         初始配置檔"

#: src/main.cc:122
msgid ""
"  --geometry WIDTHxHEIGHT+XOFF+YOFF\n"
"                                   set the size and position of main window"
msgstr ""
"  --geometry WIDTHxHEIGHT+XOFF+YOFF\n"
"                                   設定主視窗的位址和大小"

#: src/main.cc:123
msgid ""
"  --thread-limit-override NUM      if you want more simultaneous downloads..."
msgstr "  --thread-limit-override NUM      如果你需要多一點的同步下載"

#: src/main.cc:125
msgid ""
"Control Options:\n"
" the following options are effective only when another aria is already "
"running"
msgstr ""
"控制選項:\n"
" 下面的選項只有在另一個 Aria 已經在執行中時才有效"

#: src/main.cc:127
msgid "  -s, --start                      start all downloads"
msgstr "  -s, --start                      開始全部的下載工作"

#: src/main.cc:128
msgid "  -p, --stop                       stop all downloads"
msgstr "  -p, --stop                       停止全部的下載工作"

#: src/main.cc:129
msgid "  -a, --again                      re-download all items"
msgstr "  -a, --again                      重新下載所有項目"

#: src/main.cc:130
msgid "  -q, --quit                       quit Aria(with no confirmation)"
msgstr "  -q, --quit                       結束 Aria (不用確認)"

#: src/main.cc:131
msgid "  --list-name                      show the name of the current list"
msgstr "  --list-name                      顯示現在列表的名稱"

#: src/main.cc:132
msgid "  --list-prev                      switch to the previous list"
msgstr "  --list-prev                      切換到上一個列表"

#: src/main.cc:133
msgid "  --list-next                      switch to the next list"
msgstr "  --list-next                      切換到下一個列表"

#: src/main.cc:134
msgid "  --list-delete                    delete the current list"
msgstr "  --list-delete                    刪除現在的列表"

#: src/main.cc:135
msgid "  --clear-complete                 clear downloaded items"
msgstr "  --clear-complete                 清除下載完成的項目"

#: src/main.cc:136
msgid "  --ls-complete                    show URLs downloaded"
msgstr "  --ls-complete                    顯示下載完成項目的 URL"

#: src/main.cc:137
msgid "  --ls-ready                       show URLs waiting in queue"
msgstr "  --ls-ready                       顯示在佇列等待項目的 URL"

#: src/main.cc:138
msgid "  --ls-error                       show URLs in error"
msgstr "  --ls-error                       顯示錯誤項目的 URL"

#: src/main.cc:139
msgid "  --ls-run                         show URLs now downloading"
msgstr "  --ls-run                         顯示下載中項目的 URL"

#: src/main.cc:140
msgid "  --ls-stop                        show URLs stopped"
msgstr "  --ls-stop                        顯示已停止項目的 URL"

#: src/main.cc:141
msgid "  --ls-lock                        show URLs locked"
msgstr "  --ls-lock                        顯示已鎖定項目的 URL"

#: src/main.cc:142
msgid "  --ls-all                         show all URLs in queue"
msgstr "  --ls-all                         顯示所有在佇列項目的 URL"

