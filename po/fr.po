#: src/main.cc:108
msgid "Usage: aria [OPTION]..."
msgstr "Usage : aria [OPTION].."

#: src/main.cc:109
msgid "Yet another download tool"
msgstr "L'autre assistant de téléchargement"

#. command line options
#: src/main.cc:112
msgid "Options:"
msgstr "Options :"

#: src/main.cc:113
msgid "  -g, --get URL                    URL to get"
msgstr "  -g, --get URL                    URL à downloader"

#: src/main.cc:114
msgid "  -d, --dir SAVEDIR                save directory"
msgstr "  -d, --dir REP_ENREG              répertoire d'enregistrement"

#: src/main.cc:115
msgid "  -r, --ref REFERER                referer"
msgstr "  -r, --ref REFERER                'referer'"

#: src/main.cc:116
msgid "  -u, --url-list FILE              open URL list"
msgstr "  -u, --url-list FICHIER           fichier-liste d'URL à ouvrir"

#: src/main.cc:117
msgid "  -c, --crc-list FILE              open CRC list"
msgstr "  -c, --crc-list FICHIER           fichier-liste de CRC à ouvrir"

#: src/main.cc:118
msgid "  --basket                         Show basket and iconify Aria"
msgstr "  --basket                         ouvrir le panier et icônifier Aria"

#: src/main.cc:119
msgid "  -v, --version                    print version information"
msgstr "  -v, --version                    afficher le numéro de version"

#: src/main.cc:120
msgid "  -h, --help                       print this help"
msgstr "  -h, --help                       afficher cette aide"

#: src/main.cc:121
msgid "  --initrc                         initialize resource files"
msgstr "  --initrc                         recréer les fichiers de ressource"

#: src/main.cc:122
msgid ""
"  --geometry WIDTHxHEIGHT+XOFF+YOFF\n"
"                                   set the size and position of main window"
msgstr ""
"  --geometry LARGEURxHAUTEUR+POSX+POSY\n"
"                                   positionne et dimensionne la fenêtre principale"

#: src/main.cc:123
msgid ""
"  --thread-limit-override NUM      if you want more simultaneous downloads..."
msgstr "  --thread-limit-override NUM      nombre de téléchargements simultanés"

#: src/main.cc:125
msgid ""
"Control Options:\n"
" the following options are effective only when another aria is already "
"running"
msgstr ""
"Options de contrôle :\n"
" les options suivantes ne sont effectives que quand Aria est déjà ouvert"

#: src/main.cc:127
msgid "  -s, --start                      start all downloads"
msgstr "  -s, --start                      démarrer tous les téléchargements"

#: src/main.cc:128
msgid "  -p, --stop                       stop all downloads"
msgstr "  -p, --stop                       arrêter tous les téléchargements"

#: src/main.cc:129
msgid "  -a, --again                      re-download all items"
msgstr "  -a, --again                      recommencer tous les téléchargements"

#: src/main.cc:130
msgid "  -q, --quit                       quit Aria(with no confirmation)"
msgstr "  -q, --quit                       quitter Aria (sans confirmation)"

#: src/main.cc:131
msgid "  --list-name                      show the name of the current list"
msgstr "  --list-name                      afficher le nom de la liste active"

#: src/main.cc:132
msgid "  --list-prev                      switch to the previous list"
msgstr "  --list-prev                      activer la liste précédente"

#: src/main.cc:133
msgid "  --list-next                      switch to the next list"
msgstr "  --list-next                      activer la liste suivante"

#: src/main.cc:134
msgid "  --list-delete                    delete the current list"
msgstr "  --list-delete                    supprimer la liste active"

#: src/main.cc:135
msgid "  --clear-complete                 clear downloaded items"
msgstr "  --clear-complete                 supprimer les téléchargements terminés"

#: src/main.cc:136
msgid "  --ls-complete                    show URLs downloaded"
msgstr "  --ls-complete                    afficher les URL téléchargées"

#: src/main.cc:137
msgid "  --ls-ready                       show URLs waiting in queue"
msgstr "  --ls-ready                       afficher les URL en attente de téléchargement"

#: src/main.cc:138
msgid "  --ls-error                       show URLs in error"
msgstr "  --ls-error                       afficher les URL en erreur"

#: src/main.cc:139
msgid "  --ls-run                         show URLs now downloading"
msgstr "  --ls-run                         afficher les URL en cours de téléchargement"

#: src/main.cc:140
msgid "  --ls-stop                        show URLs stopped"
msgstr "  --ls-stop                        afficher les URL stoppées"

#: src/main.cc:141
msgid "  --ls-lock                        show URLs locked"
msgstr "  --ls-lock                        afficher les URL verrouillées"

#: src/main.cc:142
msgid "  --ls-all                         show all URLs in queue"
msgstr "  --ls-all                         afficher toutes les URL"

