#: src/main.cc:113
msgid "  -g, --get URL                    URL to get"
msgstr "  -g, --get URL                    закачать адрес"

# src/main.cc:91
#: src/main.cc:114
msgid "  -d, --dir SAVEDIR                save directory"
msgstr "  -d, --dir SAVEDIR                директория для сохранения"

# src/main.cc:92
#: src/main.cc:115
msgid "  -r, --ref REFERER                referer"
