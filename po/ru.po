#: src/main.cc:120
msgid "  -h, --help                       print this help"
msgstr "  -h, --help                       этот экран помощи"

# src/main.cc:97
#: src/main.cc:121
msgid "  --initrc                         initialize resource files"
msgstr "  --initrc                         инициализовать файл ресурсов"

#: src/main.cc:122
msgid ""
"  --geometry WIDTHxHEIGHT+XOFF+YOFF\n"
"                                   set the size and position of main window"
msgstr ""
"  --geometry WIDTHxHEIGHT+XOFF+YOFF\n"
"                                   установить размер и положение главного "
"окна"

# src/main.cc:99
#: src/main.cc:123
msgid ""
"  --thread-limit-override NUM      if you want more simultaneous downloads..."
msgstr "  --thread-limit-override NUM      количество одновременных закачек..."

# src/main.cc:101
#: src/main.cc:125
msgid ""
"Control Options:\n"
" the following options are effective only when another aria is already "
"running"
msgstr ""
"Опции управления:\n"
" следующие опции действуют только когда другой процесс программы запущен"

# src/main.cc:103
#: src/main.cc:127
msgid "  -s, --start                      start all downloads"
msgstr "  -s, --start                      запустить все закачки"

# src/main.cc:104
#: src/main.cc:128
msgid "  -p, --stop                       stop all downloads"
msgstr "  -p, --stop                       остановить все закачки"

# src/main.cc:105
#: src/main.cc:129
msgid "  -a, --again                      re-download all items"
msgstr "  -a, --again                      перезакачать все файлы"

# src/main.cc:106
#: src/main.cc:130
msgid "  -q, --quit                       quit Aria(with no confirmation)"
msgstr ""
"  -q, --quit                       выйти из программы (без подтверждений)"

# src/main.cc:107
#: src/main.cc:131
msgid "  --list-name                      show the name of the current list"
msgstr "  --list-name                      показать имя текущего списка"

# src/main.cc:108
#: src/main.cc:132
msgid "  --list-prev                      switch to the previous list"
msgstr "  --list-prev                      переключиться на предыдущий список"

# src/main.cc:109
#: src/main.cc:133
msgid "  --list-next                      switch to the next list"
msgstr "  --list-next                      переключиться на следующий список"

# src/main.cc:110
#: src/main.cc:134
msgid "  --list-delete                    delete the current list"
msgstr "  --list-delete                    удалить текущий список"

# src/main.cc:111
#: src/main.cc:135
msgid "  --clear-complete                 clear downloaded items"
msgstr "  --clear-complete                 удалить закачаные адреса"

# src/main.cc:112
#: src/main.cc:136
msgid "  --ls-complete                    show URLs downloaded"
msgstr "  --ls-complete                    показать закачанные адреса"

# src/main.cc:113
#: src/main.cc:137
msgid "  --ls-ready                       show URLs waiting in queue"
msgstr "  --ls-ready                       показать адреса в очереди"

# src/main.cc:114
#: src/main.cc:138
msgid "  --ls-error                       show URLs in error"
msgstr "  --ls-error                       показать адреса с ошибками"

# src/main.cc:115
#: src/main.cc:139
msgid "  --ls-run                         show URLs now downloading"
msgstr "  --ls-run                         показать закачиваемые адреса"

# src/main.cc:116
#: src/main.cc:140
msgid "  --ls-stop                        show URLs stopped"
msgstr "  --ls-stop                        показать остановленные адреса"

# src/main.cc:117
#: src/main.cc:141
msgid "  --ls-lock                        show URLs locked"
msgstr "  --ls-lock                        показать заблокированные адреса"

# src/main.cc:118
#: src/main.cc:142
msgid "  --ls-all                         show all URLs in queue"
msgstr "  --ls-all                         показать всю очередь"

