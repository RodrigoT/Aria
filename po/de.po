#: src/main.cc:108
msgid "Usage: aria [OPTION]..."
msgstr "Aufruf: aria [OPTION]"

#: src/main.cc:109
msgid "Yet another download tool"
msgstr "Ein weiteres Download-Programm"

#. command line options
#: src/main.cc:112
msgid "Options:"
msgstr "Optionen:"

#: src/main.cc:113
msgid "  -g, --get URL                    URL to get"
msgstr "  -g, --get URL                    abzurufende URL"

#: src/main.cc:114
msgid "  -d, --dir SAVEDIR                save directory"
msgstr "  -d, --dir SAVEDIR                Sicherungsverzeichnis"

#: src/main.cc:115
msgid "  -r, --ref REFERER                referer"
msgstr "  -r, --ref REFERER                Bezugslink"

#: src/main.cc:116
msgid "  -u, --url-list FILE              open URL list"
msgstr "  -u, --url-list FILE              öffne URL-Liste"

#: src/main.cc:117
msgid "  -c, --crc-list FILE              open CRC list"
msgstr "  -c, --crc-list FILE              öffne CRC-Liste"

#: src/main.cc:118
msgid "  --basket                         Show basket and iconify Aria"
msgstr "  --basket                         Zeige Korb und ikonifiziere Aria"

#: src/main.cc:119
msgid "  -v, --version                    print version information"
msgstr "  -v, --version                    zeige Versions-Information"

#: src/main.cc:120
msgid "  -h, --help                       print this help"
msgstr "  -h, --help                       zeige diese Hilfe"

#: src/main.cc:121
msgid "  --initrc                         initialize resource files"
msgstr "  --initrc                         Ressourcen-Dateien initialisieren"

#: src/main.cc:122
msgid ""
"  --geometry WIDTHxHEIGHT+XOFF+YOFF\n"
"                                   set the size and position of main window"
msgstr ""
"  --geometry WIDTHxHEIGHT+XOFF+YOFF\n"
"                                   Größe und Position des Hauptfensters "
"setzen"

#: src/main.cc:123
msgid ""
"  --thread-limit-override NUM      if you want more simultaneous downloads..."
msgstr ""
"  --thread-limit-override NUM      wenn sie mehrere simultane Abrufe "
"möchten..."

#: src/main.cc:125
msgid ""
"Control Options:\n"
" the following options are effective only when another aria is already "
"running"
msgstr ""
"Kontroll-Optionen:\n"
" folgende Optionen haben nur Wirkung, wenn bereits eine Aria-Instanz läuft"

#: src/main.cc:127
msgid "  -s, --start                      start all downloads"
msgstr "  -s, --start                      startet alle Abrufe"

#: src/main.cc:128
msgid "  -p, --stop                       stop all downloads"
msgstr "  -p, --stop                       stoppt alle Abrufe"

#: src/main.cc:129
msgid "  -a, --again                      re-download all items"
msgstr "  -a, --again                      alle Einträge erneut abrufen"

#: src/main.cc:130
msgid "  -q, --quit                       quit Aria(with no confirmation)"
msgstr "  -q, --quit                       Aria beenden(mit ohne Bestätigung;)"

#: src/main.cc:131
msgid "  --list-name                      show the name of the current list"
msgstr "  --list-name                      zeige den Namen der aktuellen Liste"

#: src/main.cc:132
msgid "  --list-prev                      switch to the previous list"
msgstr "  --list-prev                      Wechsel zur vorhergehenden Liste"

#: src/main.cc:133
msgid "  --list-next                      switch to the next list"
msgstr "  --list-next                      Wechsel zur nächsten Liste"

#: src/main.cc:134
msgid "  --list-delete                    delete the current list"
msgstr "  --list-delete                    lösche die aktuelle Liste"

#: src/main.cc:135
msgid "  --clear-complete                 clear downloaded items"
msgstr "  --clear-complete                 lösche abgerufene Einträge"

#: src/main.cc:136
msgid "  --ls-complete                    show URLs downloaded"
msgstr "  --ls-complete                    zeige abgerufene URLs"

#: src/main.cc:137
msgid "  --ls-ready                       show URLs waiting in queue"
msgstr "  --ls-ready                       zeige URLs in Warteschlange"

#: src/main.cc:138
msgid "  --ls-error                       show URLs in error"
msgstr "  --ls-error                       zeige URLs im  Fehlerstatus"

#: src/main.cc:139
msgid "  --ls-run                         show URLs now downloading"
msgstr "  --ls-run                         zeige z. Zt. abgerufene URLs"

#: src/main.cc:140
msgid "  --ls-stop                        show URLs stopped"
msgstr "  --ls-stop                        zeige gestoppte URLs"

#: src/main.cc:141
msgid "  --ls-lock                        show URLs locked"
msgstr "  --ls-lock                        zeige gesperrte URLs"

#: src/main.cc:142
msgid "  --ls-all                         show all URLs in queue"
msgstr ""
"  --ls-all                         zeige alle URLs in der Warteschlange"

