#: src/main.cc:108
msgid "Usage: aria [OPTION]..."
msgstr "Használat: aria [Paraméterek]"

#: src/main.cc:109
msgid "Yet another download tool"
msgstr "Megint egy újfajta letöltõ program"

#. command line options
#: src/main.cc:112
msgid "Options:"
msgstr "Beállítások"

#: src/main.cc:113
msgid "  -g, --get URL                    URL to get"
msgstr "  -g, --get URL\t\tletöltendõ URL"

#: src/main.cc:114
msgid "  -d, --dir SAVEDIR                save directory"
msgstr "  -d, --dir CÉLKÖNYVTÁR\t\tkönyvtár ahová menteni szeretnéd"

#: src/main.cc:115
msgid "  -r, --ref REFERER                referer"
msgstr "  -r, --ref HIVATKOZÁS\t\thivatkozás, kitõl jött a kérés"

#: src/main.cc:116
msgid "  -u, --url-list FILE              open URL list"
msgstr "  -u, --url-list FÁJL\t\tURL(cím) lista megnyitása"

#: src/main.cc:117
msgid "  -c, --crc-list FILE              open CRC list"
msgstr "  -c, --crc-list FÁJL\t\tCRC lista megnyitása"

#: src/main.cc:118
msgid "  --basket                         Show basket and iconify Aria"
msgstr "  --basket\t\tD'n'D szemetes megnyitása, Aria ikonizálása"

#: src/main.cc:119
msgid "  -v, --version                    print version information"
msgstr "  -v, --version\t\tkiírja a program verziószámát"

#: src/main.cc:120
msgid "  -h, --help                       print this help"
msgstr "  -h, --help\t\tkiírja ezt a segédletet"

#: src/main.cc:121
msgid "  --initrc                         initialize resource files"
msgstr "  --initrc\t\tinicializálja az erõforrás fájlokat"

#: src/main.cc:122
msgid ""
"  --geometry WIDTHxHEIGHT+XOFF+YOFF\n"
"                                   set the size and position of main window"
msgstr ""
"  --geometry WIDTHxHEIGHT+XOFF+YOFF\n"
"\t\ta fõablak méretét és pozícióját határozza meg"

#: src/main.cc:123
msgid ""
"  --thread-limit-override NUM      if you want more simultaneous downloads..."
msgstr ""
"  --thread-limit-override NUM      ha több egyidejû letöltést szeretnél"

#: src/main.cc:125
msgid ""
"Control Options:\n"
" the following options are effective only when another aria is already "
"running"
msgstr ""
"Vezérlõ paraméterek:\n"
" a köv. beállításoknak csak akkor van hatása,ha már fut egy Aria"

#: src/main.cc:127
msgid "  -s, --start                      start all downloads"
msgstr "  -s, --start\t\tösszes letöltés indítása"

#: src/main.cc:128
msgid "  -p, --stop                       stop all downloads"
msgstr "  -p, --stop\t\tösszes letöltés leállítása"

#: src/main.cc:129
msgid "  -a, --again                      re-download all items"
msgstr "  -a, --again\t\taz összes elem újra letöltése"

#: src/main.cc:130
msgid "  -q, --quit                       quit Aria(with no confirmation)"
msgstr "  -q, --quit\t\tkilépés az Ariából(nem kér megerõsítést)"

#: src/main.cc:131
msgid "  --list-name                      show the name of the current list"
msgstr "  --list-name\t\tkiírja az aktuális lista nevét"

#: src/main.cc:132
msgid "  --list-prev                      switch to the previous list"
msgstr "  --list-prev\t\tátváltás az elõzõ listába"

#: src/main.cc:133
msgid "  --list-next                      switch to the next list"
msgstr "  --list-next\t\tátváltás a következõ listába"

#: src/main.cc:134
msgid "  --list-delete                    delete the current list"
msgstr "  --list-delete\t\taz aktuális lista törlése"

#: src/main.cc:135
msgid "  --clear-complete                 clear downloaded items"
msgstr "  --clear-complete\tbefejezett letöltések törlése"

#: src/main.cc:136
msgid "  --ls-complete                    show URLs downloaded"
msgstr "  --ls-complete\t\tmutasd a letöltött URLeket"

#: src/main.cc:137
msgid "  --ls-ready                       show URLs waiting in queue"
msgstr "  --ls-ready\t\tmutasd letöltendõ URLeket"

#: src/main.cc:138
msgid "  --ls-error                       show URLs in error"
msgstr "  --ls-error\t\tmutasd azokat a hibás URLeket"

#: src/main.cc:139
msgid "  --ls-run                         show URLs now downloading"
msgstr "  --ls-run\t\tmutasd a letöltési folyamatban levõ URLeket"

#: src/main.cc:140
msgid "  --ls-stop                        show URLs stopped"
msgstr "  --ls-stop\t\tmutasd a leállított URLeket"

#: src/main.cc:141
msgid "  --ls-lock                        show URLs locked"
msgstr "  --ls-lock\t\tmutasd a lezárt URLeket"

#: src/main.cc:142
msgid "  --ls-all                         show all URLs in queue"
msgstr "  --ls-all\t\tmutasd a letöltésre váró URLeket"

